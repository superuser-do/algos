   "source": [
    "def edgesAwayFromTree(root):\n",
    "    count = 0\n",
    "    visited = set()\n",
    "    def dfs(node):\n",
    "        if node in visited:\n",
    "            count += 1\n",
    "            return\n",
    "        visited.add(node)\n",
    "        for child in node.children:\n",
    "            dfs(child)\n",
    "    dfs(root)\n",
    "    return count"
   ]
