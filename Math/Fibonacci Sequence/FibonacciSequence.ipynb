   "source": [
    "def fib(n):\n",
    "    memo = [0, 1, 1] + [None] * (n-1)\n",
    "    if n <= 2: return memo[n]\n",
    "    for i in range(3, n+1):\n",
    "        memo[i] = memo[i-1] + memo[i-2]\n",
    "    return memo[n]"
   ]
  },
