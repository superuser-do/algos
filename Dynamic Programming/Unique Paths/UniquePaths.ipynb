    "def uniquePaths(m, n):\n",
    "    row = [1] * n\n",
    "\n",
    "    for i in range(m - 1):\n",
    "        newRow = [1] * n\n",
    "        for j in range(n - 2, -1, -1):\n",
    "            newRow[j] = newRow[j + 1] + row[j]\n",
    "        row = newRow\n",
    "    return row[0]"
